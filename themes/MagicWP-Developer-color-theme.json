{
	"name": "MagicWP Indigo Vibes",
	"type": "dark",
	"colors": {
	  // Base UI colors with deeper indigo influence
	  "editor.background": "#1a1b3b",
	  "editor.foreground": "#f8fafc",
	  "editor.lineHighlightBackground": "#2d2b55",
	  "editor.selectionBackground": "#3c417e",
	  "editorLineNumber.foreground": "#a599e9",
	  "editorLineNumber.activeForeground": "#e7de79",
	  "editorCursor.foreground": "#fecb00",
	  "editorWhitespace.foreground": "#ffffff33",
	  
	  // Activity bar (left sidebar icons)
	  "activityBar.background": "#13143a",
	  "activityBar.foreground": "#a599e9",
	  "activityBar.activeBorder": "#fecb00",
	  "activityBar.inactiveForeground": "#7d76b7",
	  "activityBarBadge.background": "#fecb00",
	  "activityBarBadge.foreground": "#13143a",
	  
	  // Sidebar (file explorer)
	  "sideBar.background": "#151632",
	  "sideBar.foreground": "#a599e9",
	  "sideBarTitle.foreground": "#fecb00",
	  "sideBarSectionHeader.background": "#1e1e4b",
	  "sideBarSectionHeader.foreground": "#fecb00",
	  "list.activeSelectionBackground": "#2d2b55",
	  "list.activeSelectionForeground": "#ffffff",
	  "list.hoverBackground": "#222246",
	  "list.inactiveSelectionBackground": "#222246",
	  "list.highlightForeground": "#fecb00",
	  
	  // Title and menu bar
	  "titleBar.activeBackground": "#13143a",
	  "titleBar.activeForeground": "#f8fafc",
	  "menubar.selectionBackground": "#2d2b55",
	  "menubar.selectionForeground": "#ffffff",
	  
	  // Status bar (bottom)
	  "statusBar.background": "#13143a",
	  "statusBar.foreground": "#a599e9",
	  "statusBar.noFolderBackground": "#13143a",
	  "statusBar.debuggingBackground": "#fecb00",
	  "statusBar.debuggingForeground": "#13143a",
	  
	  // Tabs
	  "tab.activeBackground": "#2d2b55",
	  "tab.activeForeground": "#ffffff",
	  "tab.inactiveBackground": "#1e1e3f",
	  "tab.inactiveForeground": "#a599e9",
	  "tab.activeBorder": "#fecb00",
	  "tab.activeBorderTop": "#fecb00",
	  "tab.hoverBackground": "#2d2b55",
	  "editorGroupHeader.tabsBackground": "#101127",
	  
	  // Panels and terminal
	  "panel.background": "#151632",
	  "panelTitle.activeBorder": "#fecb00",
	  "panelTitle.activeForeground": "#ffffff",
	  "panelTitle.inactiveForeground": "#a599e9",
	  "terminal.background": "#151632",
	  "terminal.foreground": "#ffffff",
	  "terminal.ansiBlue": "#82aaff",
	  "terminal.ansiCyan": "#80ffea",
	  "terminal.ansiGreen": "#3ad900",
	  "terminal.ansiMagenta": "#ff2c70",
	  "terminal.ansiRed": "#ec3a37",
	  "terminal.ansiYellow": "#fecb00",
	  
	  // Dropdown and input
	  "dropdown.background": "#1e1e3f",
	  "dropdown.foreground": "#f8fafc",
	  "input.background": "#2d2b55",
	  "input.foreground": "#f8fafc",
	  "input.placeholderForeground": "#a599e9",
	  "inputOption.activeBorder": "#fecb00",
	  
	  // Scrollbar
	  "scrollbarSlider.background": "#a599e980",
	  "scrollbarSlider.hoverBackground": "#a599e9aa",
	  "scrollbarSlider.activeBackground": "#fecb0080",
	  
	  // Other UI elements
	  "badge.background": "#fecb00",
	  "badge.foreground": "#13143a",
	  "breadcrumb.foreground": "#a599e9",
	  "breadcrumb.focusForeground": "#f8fafc",
	  "breadcrumb.activeSelectionForeground": "#fecb00",
	  "button.background": "#fecb00",
	  "button.foreground": "#13143a",
	  "button.hoverBackground": "#ffee00",
	  "diffEditor.insertedTextBackground": "#3ad90020",
	  "diffEditor.removedTextBackground": "#ff2c7020",
	  "progressBar.background": "#fecb00",
	  "peekViewEditor.background": "#1e1e3f",
	  "editorWidget.background": "#1e1e3f",
	  "editorWidget.border": "#fecb00",
	  "editorSuggestWidget.background": "#1e1e3f",
	  
	  // Welcome page
	  "welcomePage.background": "#1a1b3b",
	  
	  // Git decorations
	  "gitDecoration.addedResourceForeground": "#3ad900",
	  "gitDecoration.modifiedResourceForeground": "#fecb00",
	  "gitDecoration.deletedResourceForeground": "#ff2c70",
	  "gitDecoration.untrackedResourceForeground": "#a599e9",
	  "gitDecoration.ignoredResourceForeground": "#7d76b7",
	  "gitDecoration.conflictingResourceForeground": "#ec3a37"
	},
	"tokenColors": [
	  {
		"name": "Comments",
		"scope": ["comment", "punctuation.definition.comment"],
		"settings": {
		  "foreground": "#b362ff",
		  "fontStyle": "italic"
		}
	  },
	  {
		"name": "Function declarations",
		"scope": ["entity.name.function", "support.function"],
		"settings": {
		  "foreground": "#fed763",
		}
	  },
	  {
		"name": "Function calls",
		"scope": ["meta.function-call.generic", "variable.function"],
		"settings": {
		  "foreground": "#95fcfc"
		}
	  },
	  {
		"name": "Variables",
		"scope": ["variable", "variable.parameter"],
		"settings": {
		  "foreground": "#e258bb"
		}
	  },
	  {
		"name": "PHP Variables",
		"scope": ["variable.other.php", "variable.language.this.php"],
		"settings": {
		  "foreground": "#8bf3fb"
		}
	  },
	  {
		"name": "Keywords and control",
		"scope": ["keyword", "keyword.control", "keyword.operator.logical", "keyword.operator.comparison"],
		"settings": {
		  "foreground": "#f1ab3a"
		}
	  },
	  {
		"name": "Operators and comparison",
		"scope": ["keyword.operator", "punctuation.separator", "punctuation.terminator"],
		"settings": {
		  "foreground": "#fa7c06"
		}
	  },
	  {
		"name": "Storage and types",
		"scope": ["storage", "storage.type", "storage.modifier", "support.type"],
		"settings": {
		  "foreground": "#ff9d00",
		  "fontStyle": "bold"
		}
	  },
	  {
		"name": "Classes and types",
		"scope": ["entity.name.class", "entity.name.type.class", "entity.name.type"],
		"settings": {
		  "foreground": "#59d2bd",
		  "fontStyle": "bold"
		}
	  },
	  {
		"name": "Strings",
		"scope": ["string", "punctuation.definition.string"],
		"settings": {
		  "foreground": "#77f097"
		}
	  },
	  {
		"name": "Constants, numbers, booleans",
		"scope": ["constant", "constant.numeric", "constant.language", "constant.character", "constant.other"],
		"settings": {
		  "foreground": "#d6c86d"
		}
	  },
	  {
		"name": "Array keys, object properties",
		"scope": ["meta.object-literal.key", "variable.object.property", "support.variable.property"],
		"settings": {
		  "foreground": "#9effff"
		}
	  },
	  {
		"name": "HTML Tags",
		"scope": ["entity.name.tag", "punctuation.definition.tag"],
		"settings": {
		  "foreground": "#f43471"
		}
	  },
	  {
		"name": "Attributes",
		"scope": ["entity.other.attribute-name"],
		"settings": {
		  "foreground": "#77e6f9",
		  "fontStyle": "italic"
		}
	  },
	  {
		"name": "CSS Properties",
		"scope": ["support.type.property-name"],
		"settings": {
		  "foreground": "#47f197",
		  "fontStyle": ""
		}
	  },
	  {
		"name": "CSS Values",
		"scope": ["support.constant.property-value"],
		"settings": {
		  "foreground": "#a499fc"
		}
	  },
	  {
		"name": "CSS Units",
		"scope": ["keyword.other.unit"],
		"settings": {
		  "foreground": "#afa45b"
		}
	  },
	  {
		"name": "WordPress Specific",
		"scope": [
		  "support.function.wordpress",
		  "meta.function-call.php support.function.wordpress"
		],
		"settings": {
		  "foreground": "#82aaff",
		  "fontStyle": "bold"
		}
	  },
	  {
		"name": "WordPress Hook Functions",
		"scope": [
		  "support.function.wordpress.hook",
		  "meta.function-call.php entity.name.function.wordpress.hook"
		],
		"settings": {
		  "foreground": "#c792ea",
		  "fontStyle": "bold italic"
		}
	  },
	  {
		"name": "PHP Array Keys and Object Properties",
		"scope": ["meta.array.php string", "variable.other.property.php"],
		"settings": {
		  "foreground": "#4ae6a2"
		}
	  },

	  {
		"name": "JSON Properties",
		"scope": ["support.type.property-name.json"],
		"settings": {
		  "foreground": "#ff7edb"
		}
	  },
	  {
		"name": "JavaScript this",
		"scope": ["variable.language.this"],
		"settings": {
		  "foreground": "#ff2c70",
		  "fontStyle": "italic"
		}
	  },
	  {
		"name": "TypeScript Types",
		"scope": ["entity.name.type.interface.ts", "entity.name.type.ts"],
		"settings": {
		  "foreground": "#80ffea",
		  "fontStyle": "bold"
		}
	  },
	  {
		"name": "TypeScript Interfaces",
		"scope": ["entity.name.type.interface"],
		"settings": {
		  "foreground": "#80ffea",
		  "fontStyle": "bold italic"
		}
	  },
	  {
		"name": "TypeScript Decorators",
		"scope": ["meta.decorator", "variable.annotation"],
		"settings": {
		  "foreground": "#c792ea",
		  "fontStyle": "italic"
		}
	  },
	  {
		"name": "Markdown Headings",
		"scope": ["markup.heading"],
		"settings": {
		  "foreground": "#fecb00",
		  "fontStyle": "bold"
		}
	  },
	  {
		"name": "Markdown Links",
		"scope": ["markup.underline.link"],
		"settings": {
		  "foreground": "#82aaff",
		  "fontStyle": "underline"
		}
	  },
	  {
		"name": "Markdown Bold",
		"scope": ["markup.bold"],
		"settings": {
		  "foreground": "#ffee80",
		  "fontStyle": "bold"
		}
	  },
	  {
		"name": "Markdown Italic",
		"scope": ["markup.italic"],
		"settings": {
		  "foreground": "#ff7edb",
		  "fontStyle": "italic"
		}
	  },
	  {
		"name": "Markdown Code",
		"scope": ["markup.inline.raw", "markup.fenced_code"],
		"settings": {
		  "foreground": "#3ad900"
		}
	  }
	],
	"semanticHighlighting": true,
	"semanticTokenColors": {
	  "property": "#9effff",
	  "function": "#fecb00",
	  "variable.defaultLibrary": "#c792ea",
	  "class": "#80ffea",
	  "enumMember": "#ffee80"
	}
  }